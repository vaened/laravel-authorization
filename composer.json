{
  "name": "enea/laravel-authorization",
  "description": "Package to manage the permissions in a laravel application",
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "enea dhack",
      "email": "enea.so@live.com"
    }
  ],
  "keywords": [
    "enea",
    "laravel",
    "permission",
    "authorization",
    "acl",
    "security"
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^v10.48.18",
    "psr/log": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.0",
    "mockery/mockery": "^1.0",
    "laravel/legacy-factories": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Enea\\Authorization\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Enea\\Authorization\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Enea\\Authorization\\AuthorizationServiceProvider"
      ]
    }
  }
}
